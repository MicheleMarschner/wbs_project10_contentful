:15},"source":"../src/components/posts/Posts.jsx","original":{"line":57,"column":21}},{"generated":{"line":80,"column":0},"source":"../src/components/posts/Posts.jsx","original":{"line":57,"column":6}},{"name":"renderPosts","generated":{"line":80,"column":5},"source":"../src/components/posts/Posts.jsx","original":{"line":57,"column":30}},{"generated":{"line":80,"column":16},"source":"../src/components/posts/Posts.jsx","original":{"line":57,"column":41}},{"generated":{"line":80,"column":18},"source":"../src/components/posts/Posts.jsx","original":{"line":57,"column":6}},{"generated":{"line":80,"column":19},"source":"../src/components/posts/Posts.jsx","original":{"line":54,"column":4}},{"generated":{"line":80,"column":20},"source":"../src/components/posts/Posts.jsx","original":{"line":53,"column":2}},{"generated":{"line":81,"column":0},"source":"../src/components/posts/Posts.jsx","original":{"line":60,"column":1}},{"generated":{"line":82,"column":0},"source":"../src/components/posts/Posts.jsx","original":{"line":62,"column":0}},{"generated":{"line":87,"column":0},"source":"../src/components/posts/Posts.jsx","original":{"line":68,"column":0}}],"sources":{"../src/components/posts/Posts.jsx":"//!!To DO: insert card expanding effect\r\n// insert canvas that inserts white triangle\r\n//overlay over image with header --> from bootstrap cookbook\r\n\r\nimport React from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nimport { usePosts } from \"../../custom-hooks/\";\r\n//readableDate transforms the Date from original format \"2017-05-12T00:00+02:00\"\r\nimport \"./Posts.css\";\r\n\r\nexport default function Posts() {\r\n  const [posts, isLoading] = usePosts(); // [response.items, false];\r\n  posts.map(ele => console.log(ele.heroImage));\r\n\r\n  const readableDate = dateString => new Date(dateString).toDateString();\r\n\r\n  const renderPosts = () => {\r\n    if (isLoading) return <p>Loading...</p>;\r\n    //<small> is just like <strong>\r\n    return posts.map(post => (\r\n      <div className=\"responsive-card-wrapper\">\r\n        <Link className=\"posts__post\" key={post.slug} to={post.slug===\"your-article\"? \"/addPost\" : post.slug}>\r\n          <div className=\"card responsive\">\r\n            {post.heroImage && (\r\n              <div\r\n                className=\"posts__post__img__container\"\r\n                style={{\r\n                  background: `url(${\r\n                    post.heroImage.fields.file.url\r\n                  }) no-repeat`,\r\n                  backgroundSize: \"cover\"\r\n                }}\r\n              >\r\n                <h3 className=\"p-2\">{post.title}</h3>\r\n              </div>\r\n            )}\r\n            <div>\r\n              <small\r\n                style={{ color: \"gray\", display: \"block\", textAlign: \"right\" }}\r\n              >\r\n                <em>{readableDate(post.publishDate)}</em>\r\n              </small>\r\n              <p>{post.description}</p>\r\n            </div>\r\n          </div>\r\n        </Link>\r\n      </div>\r\n    ));\r\n  };\r\n\r\n  //??needed to immediately invoke renderPosts ala ()\r\n  return (\r\n    <section className=\"posts__container\">\r\n      <h2>Articles</h2>\r\n\r\n      <div className=\"posts\">{renderPosts()}</div>\r\n    </section>\r\n  );\r\n}\r\n\r\n/*<img\r\n                    className=\"post__intro__img\"\r\n                    src={post.heroImage.fields.file.url}\r\n                    alt={post.title}\r\n                  />*/\r\n\r\n//to={post.slug === \"your-article\" ? \"/addPost\" : post.slug}\r\n"},"lineCount":null}},"error":null,"hash":"18d7d744b0829cf68694005278bdf92a","cacheData":{"env":{}}}